<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" prefHeight="328.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.saxion.re.solarsmart.SolarSmartController">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="275.0" minWidth="200.0" prefWidth="234.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="266.0" minWidth="100.0" prefWidth="141.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" vgrow="SOMETIMES" />
    </rowConstraints>

    <Label text="First Name: " GridPane.columnIndex="0" GridPane.rowIndex="0">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin></Label>
    <TextField fx:id="firstNameT" maxHeight="20.0" maxWidth="75.0">
      <GridPane.margin>
         <Insets left="80.0" />
      </GridPane.margin></TextField>

    <Label text="Last Name: " GridPane.columnIndex="0" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin></Label>
    <TextField fx:id="lastNameT" maxHeight="20.0" maxWidth="75.0" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets left="80.0" />
      </GridPane.margin></TextField>

    <Label text="Roof Width:" GridPane.columnIndex="0" GridPane.rowIndex="2">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin></Label>
    <TextField fx:id="roofWT" maxHeight="20.0" maxWidth="75.0" GridPane.rowIndex="2">
      <GridPane.margin>
         <Insets left="80.0" />
      </GridPane.margin></TextField>

    <Label text="Roof Height: " GridPane.columnIndex="0" GridPane.rowIndex="3">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin></Label>
    <TextField fx:id="roofHT" maxHeight="20.0" maxWidth="75.0" prefHeight="0.0" prefWidth="0.0" GridPane.rowIndex="3">
      <GridPane.margin>
         <Insets left="80.0" />
      </GridPane.margin></TextField>

    <Label alignment="TOP_CENTER" text="Annual power consumption: " textAlignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="4">
      <GridPane.margin>
         <Insets left="10.0" />
      </GridPane.margin></Label>
    <TextField fx:id="powerConsT" maxHeight="20.0" maxWidth="86.0" prefHeight="0.0" prefWidth="0.0" GridPane.rowIndex="4">
      <GridPane.margin>
         <Insets left="164.0" />
      </GridPane.margin></TextField>
   <Button fx:id="confirm" mnemonicParsing="false" onAction="#onConfirm" text="Confirm" GridPane.columnIndex="1" GridPane.rowIndex="3">
      <GridPane.margin>
         <Insets left="50.0" />
      </GridPane.margin>
   </Button>
   <CheckBox fx:id="noCheck" mnemonicParsing="false" text="No" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets left="50.0" />
      </GridPane.margin></CheckBox>
   <CheckBox fx:id="yesCheck" mnemonicParsing="false" text="Yes" GridPane.columnIndex="1" GridPane.rowIndex="1">
      <GridPane.margin>
         <Insets />
      </GridPane.margin>
   </CheckBox>

</GridPane>
